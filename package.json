{
  "name": "kit",
  "version": "1.0.0",
  "description": "Universal JS app scaffolding",
  "main": "src/index.js",
  "scripts": {
    "build": "./bin/build",
    "eslint": "./bin/eslint",
    "browserify": "./bin/browserify",
    "css": "./bin/css",
    "watch": "npm-watch"
  },
  "watch": {
    "eslint": {
      "patterns": ["src/*"],
      "extensions": "js",
      "ignore": ["src/universal/dist/bundle.js"]
    },
    "browserify": {
      "patterns": ["src/universal/*"],
      "extensions": "js",
      "ignore": ["src/universal/dist/bundle.js"]
    },
    "css": {
      "patterns": ["src/css/*"],
      "extensions": "scss"
    }
  },
  "repository": "https://github.com/mgrahamjo/kit",
  "author": "Mike Johnson",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "clean-css": "^3.4.21",
    "eslint": "^3.10.2",
    "npm-watch": "^0.1.6",
    "postcss-cli": "^2.6.0",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^15.0.0",
    "uglify-js": "^2.7.4"
  },
  "dependencies": {
    "oat": "^1.0.3",
    "express": "^4.14.0"
  }
}
